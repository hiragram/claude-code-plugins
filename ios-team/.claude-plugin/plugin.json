{
    "name": "ios-team",
    "description": "iOSアプリ開発チーム",
    "version": "0.2.0",
    "mcpServers": {
        "pbxproj": {
            "command": "swx",
            "args": [
                "hiragram/pbxproj-mcp"
            ]
        },
        "iossim": {
            "command": "swx",
            "args": [
                "hiragram/iossim-mcp"
            ]
        },
        "XcodeBuildMCP": {
            "command": "npx",
            "args": [
                "-y",
                "xcodebuildmcp@latest"
            ]
        },
        "firebase": {
            "command": "npx",
            "args": [
                "-y",
                "firebase-tools@latest",
                "mcp"
            ]
        }
    },
    "author": {
        "name": "hiragram"
    }
}